        {
            position:relative;
            left:-5px;
            //content:"?";
            //content:"\u2713";
        }
     </style>
     <script>
        // visible password
        function visible1() 
        {
            var x = document.getElementById("password1");
            if (x.type === "password") {
              x.type = "text";
            } else {
              x.type = "password";
            }
        }
        // check password
        var myInput1 = document.getElementById("password1");
        var sletter1 = document.getElementById("sletter1");
        var cletter1 = document.getElementById("cletter1");
        var number1 = document.getElementById("number1");
        var length1 = document.getElementById("length1");

        myInput1.onfocus = function()
        {
            document.getElementById("message1").style.display = "block";
        }
        myInput1.onblur = function()
        {
            document.getElementById("message1").style.display = "none";
        }

        myInput1.onkeyup = function()
        {
            var lowerCases1 = /[a-z]/g;
            if(myInput1.value.match(lowerCases1))
            {
                    sletter1.classList.remove("invalid1");
                    sletter1.classList.add("valid1");
            }
            else
            {
                    sletter1.classList.remove("valid1");
                    sletter1.classList.add("invalid1");
            }

            var upperCases1 = /[A-Z]/g;
            if(myInput1.value.match(upperCases1))
            {
                    cletter1.classList.remove("invalid1");
                    cletter1.classList.add("valid1");
            }
            else
            {
                    cletter1.classList.remove("valid1");
                    cletter1.classList.add("invalid1");
            }

            var numbers1 = /[0-9]/g;
            if(myInput1.value.match(numbers1))
            {
                    number1.classList.remove("invalid1");
                    number1.classList.add("valid1");
            }
            else
            {
                    number1.classList.remove("valid1");
                    number1.classList.add("invalid1");
            }

            if(myInput1.value.length>=8)
            {
                    length1.classList.remove("invalid1");
                    length1.classList.add("valid1");
            }
            else
            {
                    length1.classList.remove("valid1");
                    length1.classList.add("invalid1");
            }
        }
